@* @page "/models/{ModelName}" *@
@page "/models"
@inject NavigationManager Navigation



@if (ModelName == "chatgpt")
{
    <button class="btn btn-danger mb-2" @onclick="@( ()=>Navigation.NavigateTo("/models?modelName=dall-e"))">Swtich to Dall-E Model</button>

    <ChatGpt />

}
else
{
    <button class="btn btn-danger mb-2" @onclick="@( ()=>Navigation.NavigateTo("/models?modelName=chatgpt"))">Swtich to ChatGpt Model</button>


    <DallE />
}


@code
{
    // [Parameter]
    // public string ModelName { get; set; }
    [SupplyParameterFromQuery(Name = "modelName")]
    public string ModelName { get; set; }

}